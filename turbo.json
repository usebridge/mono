{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {},
    "dev": {
      "persistent": true,
      "cache": false
    },
    "dev:dashboard": {
      "cache": false
    },
    "dev:website": {
      "cache": false
    },
    "dev:zero-cache": {
      "cache": false
    },
    "dev:db-up": {
      "cache": false
    },
    "dev:db-down": {
      "cache": false
    },
    "dev:db-migrate": {
      "dependsOn": ["^dev:db-up"],
      "cache": false
    },
    "format": {},
    "lint": {},
    "typecheck": {
      "dependsOn": ["^typecheck"]
    }
  }
}
